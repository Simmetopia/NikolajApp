{
  "expo": {
    "name": "nikolaj-syg",
    "description": "This project is really great.",
    "slug": "nikolaj-syg",
    "privacy": "unlisted",
    "sdkVersion": "26.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#E1BEE7",
    "assetBundlePatterns": ["assets/**"],
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#f5f5f5"
    },
    "extra": {
      "expoBuildNumber": "1"
    },
    "ios": {
      "supportsTablet": false,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.bwstracking.ios",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription":
          "The app needs permission to your camera in order to take photos",
        "NSPhotoLibraryUsageDescription":
          "The app needs permission to your camera roll in order to add photos you've already taken"
      },
      "loadJSInBackgroundExperimental": true
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.bwstracking.android",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
